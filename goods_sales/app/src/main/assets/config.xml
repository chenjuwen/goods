<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!-- sdcard存储的根目录：db文件复制到该目录，文件生成存储目录 -->
	<sdcardRootPath>/sdcard/goods/</sdcardRootPath>

	<!-- SQLite数据库参数 -->
	<dbName>goods.db</dbName>
	<dbPassword></dbPassword>
	<dbVersion>1</dbVersion>

	<!-- Action类所在的根包路径，多个路径用分号(;)分隔 -->
	<actionBasePackage>com.heasy.goods.action</actionBasePackage>

	<!-- Service类所在的根包路径，多个路径用分号(;)分隔 -->
	<serviceBasePackage>com.heasy.goods</serviceBasePackage>

	<!-- webview加载html页面的根路径 -->
	<webviewLoadBasePath>file:///android_asset/html/</webviewLoadBasePath>

	<!-- webview的入口html页面 -->
	<webviewMainPage>main.html</webviewMainPage>

</root>
